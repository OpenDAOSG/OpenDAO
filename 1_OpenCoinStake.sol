contract OpenCoinStake is Context, Ownable {
    using SafeMath for uint;
    using Address for address;

    IERC20 public _aToken;
    IERC20 public _bToken;
    IUniswapV2Router02 public _uniswapV2Router;
    IUniswapV2Factory public _uniswapV2Factory;
    IUniswapV2Pair public _uniswapV2Pair; // LP

    uint public _totalLpSupply; // LP 总数量
    uint public _totalATokenSupply; // a token 总数量
    uint public _aTokenAmountLimit; // a token 到达这个值，就停止质押，0 代表不限制
    uint public _aTokenAmountLimitPer; // a token 每次质押最大限制，0 代表不限制
    uint public _totalBTokenSupply; // b token 总数量
    uint public _bTokenAmountLimit; // b token 到达这个值，就停止质押，0 代表不限制
    uint public _bTokenAmountLimitPer; // b token 每次质押最大限制，0 代表不限制
    mapping(address => uint) public _lpStakeAmount; // 用户LP数量
    mapping(address => uint) public _aTokenAmount; // 用户a token数量
    mapping(address => uint) public _bTokenAmount; // 用户b token数量

    bool private _paused = false; // 是否暂停

    address public _updateATokenTotalSupplyAddress;

    event Staked(address indexed user, uint lpAmount, uint aTokenAmount, uint bTokenAmount);
    event Withdrawn(address indexed user, uint lpAmount);

    event BackTotalATokenSupply(address indexed user, uint totalATokenSupply);
   
    constructor (
        uint aTokenAmountLimit,
        uint bTokenAmountLimit,
        uint aTokenAmountLimitPer,
        uint bTokenAmountLimitPer,
        address aTokenAddr,
        address bTokenAddr,
        address routerAddr,
        address updateATokenTotalSupplyAddress
    ) public {
        _aTokenAmountLimit = aTokenAmountLimit;
        _bTokenAmountLimit = bTokenAmountLimit;

        _aTokenAmountLimitPer = aTokenAmountLimitPer;
        _bTokenAmountLimitPer = bTokenAmountLimitPer;

        _aToken = IERC20(aTokenAddr);
        _bToken = IERC20(bTokenAddr);

        _uniswapV2Router = IUniswapV2Router02(routerAddr);

        _uniswapV2Factory = IUniswapV2Factory(_uniswapV2Router.factory());

        _uniswapV2Pair = IUniswapV2Pair(_uniswapV2Factory.getPair(aTokenAddr, bTokenAddr));

        _updateATokenTotalSupplyAddress=updateATokenTotalSupplyAddress;
    }


    // --------------------------------------------------------------

    // 质押
    function deposit(uint lpStakeAmount) public isHuman whenNotPaused {
        require(_lpStakeAmount[msg.sender] <= 0, "Repeated stake is prohibited");

        uint lpBalance = _uniswapV2Pair.balanceOf(msg.sender);
        require(lpBalance > 0, "There is no token for the address");

        require(lpStakeAmount<lpBalance , "The pledge amount exceeds the balance");

        _uniswapV2Pair.transferFrom(msg.sender, address(this), lpStakeAmount);

        _lpStakeAmount[msg.sender] = lpStakeAmount;
        _totalLpSupply = _totalLpSupply.add(lpStakeAmount);

        (uint aTokenAmount, uint bTokenAmount) = getReservesByLp(lpStakeAmount);
        _aTokenAmount[msg.sender] = aTokenAmount;
        _bTokenAmount[msg.sender] = bTokenAmount;

        _totalATokenSupply = _totalATokenSupply.add(aTokenAmount);
        _totalBTokenSupply = _totalBTokenSupply.add(bTokenAmount);

        require(!isTokenAmountLimit(), "Activities paused");
        require(!isTokenAmountLimitPer(aTokenAmount, bTokenAmount), "The amount cannot exceed the maximum limit");

        //质押信息回调
        emit Staked(msg.sender, lpStakeAmount, aTokenAmount, bTokenAmount);
    }

    //提现
    function withdraw() external {
        uint lpAmount = _lpStakeAmount[msg.sender];
        require(lpAmount > 0, "Staking amount is 0");

        _uniswapV2Pair.transfer(msg.sender, lpAmount);

        _totalLpSupply = _totalLpSupply.sub(lpAmount);
        _lpStakeAmount[msg.sender] = 0;

        //        _totalATokenSupply = _totalATokenSupply.sub(_aTokenAmount[msg.sender]);
        //        _totalBTokenSupply = _totalBTokenSupply.sub(_bTokenAmount[msg.sender]);

        _aTokenAmount[msg.sender] = 0;
        _bTokenAmount[msg.sender] = 0;

        emit Withdrawn(msg.sender, lpAmount);
    }

    // --------------------------------------------------------------

    function setTokenAmountLimit(uint aTokenAmountLimit, uint bTokenAmountLimit) public onlyOwner {
        _aTokenAmountLimit = aTokenAmountLimit;
        _bTokenAmountLimit = bTokenAmountLimit;
    }

    function isTokenAmountLimit() public view returns (bool) {
        return !(_totalATokenSupply <= _aTokenAmountLimit || _aTokenAmountLimit == 0) || !(_totalBTokenSupply <= _bTokenAmountLimit || _bTokenAmountLimit == 0);
    }

    function setTokenAmountLimitPer(uint aTokenAmountLimitPer, uint bTokenAmountLimitPer) public onlyOwner {
        _aTokenAmountLimitPer = aTokenAmountLimitPer;
        _bTokenAmountLimitPer = bTokenAmountLimitPer;
    }

    function isTokenAmountLimitPer(uint aTokenAmount, uint bTokenAmount) public view returns (bool) {
        return !(aTokenAmount <= _aTokenAmountLimitPer || _aTokenAmountLimitPer == 0) || !(bTokenAmount <= _bTokenAmountLimitPer || _bTokenAmountLimitPer == 0);
    }

    function setPaused(bool paused) public onlyOwner {
        _paused = paused;
    }

    function isPaused() public view returns (bool) {
        return _paused;
    }

    modifier whenNotPaused() {
        require(!isPaused(), "activities Paused");
        _;
    }

    modifier isHuman() {
        require(!Address.isContract(msg.sender), "sorry, human only");
        require(tx.origin == msg.sender, "sorry, human only");
        _;
    }

    function getReservesByLp(uint lpAmount) public view returns (uint, uint) {
        (uint _reserve0, uint _reserve1) = getReserves(address(_aToken), address(_bToken));
        uint totalSupply = _uniswapV2Pair.totalSupply();

        return (_reserve0.mul(lpAmount).div(totalSupply), _reserve1.mul(lpAmount).div(totalSupply));
    }

    function getReserves(address aTokenAddr, address bTokenAddr) public view returns (uint, uint) {
        (uint _reserve0, uint _reserve1,) = _uniswapV2Pair.getReserves();

        if (aTokenAddr == _uniswapV2Pair.token0()) {
            return (_reserve0, _reserve1);
        } else {
            return (_reserve1, _reserve0);
        }
    }

    modifier updateTotalSupplyOwner() {
        require(_updateATokenTotalSupplyAddress == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    function setTotalSupplyUpdateOwner(address updaterOwner) public onlyOwner{

        require(updaterOwner==address(this),"Cannot set contract address");

        _updateATokenTotalSupplyAddress = updaterOwner;

    }

    function setATokenTotalSupply(uint amount) public updateTotalSupplyOwner whenNotPaused{

        require(amount > 0, "Return amount is 0");

       // _totalATokenSupply = _totalATokenSupply.sub(amount);
        _totalATokenSupply = amount;

       emit BackTotalATokenSupply(msg.sender,amount);
    }


}